<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/activity_battle_net_dashbored"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:paddingBottom="@dimen/activity_vertical_margin"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    tools:context="com.example.jack.steamblizz.BattleNetDashbored">

    <ImageButton
        android:layout_width="200px"
        android:layout_height="200px"
        app:srcCompat="@drawable/sc2"
        android:layout_alignParentTop="true"
        android:layout_alignParentStart="true"
        android:layout_marginStart="21dp"
        android:layout_marginTop="24dp"
        android:id="@+id/starcraftimg" />

    <ImageButton
        android:layout_width="200px"
        android:layout_height="200px"
        app:srcCompat="@drawable/diablo"
        android:layout_marginTop="36dp"
        android:id="@+id/diabloimg"
        android:layout_below="@+id/starcraftimg"
        android:layout_alignStart="@+id/starcraftimg" />

    <ImageButton
        android:layout_width="200px"
        android:layout_height="200px"
        app:srcCompat="@drawable/wow"
        android:layout_below="@+id/diabloimg"
        android:layout_alignStart="@+id/diabloimg"
        android:layout_marginTop="50dp"
        android:id="@+id/wowimg" />
</RelativeLayout>
